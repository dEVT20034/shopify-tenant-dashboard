// This is a dummy Prisma schema to satisfy Vercel build checks
// The actual project uses Drizzle ORM - see src/db/schema.ts

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_UNUSED")
}

// Dummy model to satisfy schema validation
model DummyModel {
  id Int @id @default(autoincrement())
}
